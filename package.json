{
  "name": "wiki-monkey",
  "version": "4.0.0",
  "author": "Dario Giovannetti",
  "description": "MediaWiki bot and editor-assistant user script.",
  "dependencies": {
    "babel-polyfill": "",
    "hyperscript": "",
    "hyperscript-helpers": "",
    "jss": "",
    "jss-preset-default": "",
    "moment": ""
  },
  "devDependencies": {
    "babel-cli": "",
    "babel-preset-env": "",
    "babelify": "",
    "browserify": "",
    "coffeeify": "",
    "coffeelint": "",
    "coffeescript": "",
    "http-server": ""
  },
  "scripts": {
    "build": "python auxiliary/build.py",
    "serve-gencert": "cd auxiliary && sh ./gencert.sh",
    "serve": "http-server --cors --ssl --cert auxiliary/dev-cert.pem --key auxiliary/dev-key.pem"
  },
  "browserify": {
    "transform": [
      "coffeeify"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "comments": false,
    "compact": false
  }
}
