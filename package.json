{
  "name": "wiki-monkey",
  "version": "4.0.0",
  "author": "Dario Giovannetti",
  "description": "MediaWiki bot and editor-assistant user script.",
  "dependencies": {
    "hyperscript": "",
    "hyperscript-helpers": ""
  },
  "devDependencies": {
    "babel-cli": "",
    "babel-preset-env": "",
    "babelify": "",
    "browserify": "",
    "coffeeify": "",
    "coffeelint": "",
    "coffeescript": "",
    "http-server": ""
  },
  "scripts": {
    "build": "python auxiliary/build.py",
    "serve-gencert": "cd auxiliary && sh ./gencert.sh",
    "serve": "http-server --ssl --cert auxiliary/dev-cert.pem --key auxiliary/dev-key.pem"
  },
  "browserify": {
    "transform": [
      "coffeeify"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "comments": false,
    "compact": false
  },
  "coffeelintConfig": {
    "arrow_spacing": {
      "level": "error"
    },
    "braces_spacing": {
      "level": "error"
    },
    "camel_case_classes": {
      "level": "error"
    },
    "colon_assignment_spacing": {
      "level": "error",
      "spacing": {
        "left": 0,
        "right": 1
      }
    },
    "cyclomatic_complexity": {
      "level": "warn"
    },
    "duplicate_key": {
      "level": "error"
    },
    "indentation": {
      "level": "error",
      "value": 4
    },
    "max_line_length": {
      "level": "warn",
      "value": 79
    },
    "newlines_after_classes": {
      "level": "error",
      "value": 2
    },
    "no_implicit_parens": {
      "level": "error"
    },
    "no_stand_alone_at": {
      "level": "error"
    },
    "space_operators": {
      "level": "error"
    },
    "spacing_after_comma": {
      "level": "error"
    }
  }
}
